{
    "name": "gupalo/browser-notifier",
    "description": "Extension of Symfony Notifier for browsers",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "psr/log": ">=1.1",
        "symfony/notifier": "^5.4|^6.0",
        "twig/twig": "^3.3"
    },
    "require-dev": {
        "phpspec/prophecy-phpunit": "^2.0",
        "phpunit/phpunit": "^9.5"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "platform": {
            "php": "7.4"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Gupalo\\BrowserNotifier\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gupalo\\BrowserNotifier\\Tests\\": "tests/"
        }
    }
}
